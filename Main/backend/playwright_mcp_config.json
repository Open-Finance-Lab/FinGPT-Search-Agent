{
  "browser": {
    "browserName": "chromium",
    "headless": true,
    "launchOptions": {
      "executablePath": "/usr/bin/chromium-bundled",
      "args": [
        "--no-sandbox",
        "--disable-setuid-sandbox",
        "--disable-dev-shm-usage",
        "--disable-gpu"
      ]
    }
  }
}