{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": [
        "@modelcontextprotocol/server-filesystem",
        "/app"
      ],
      "disabled": false
    },
    "sec-edgar": {
      "command": "python",
      "args": [
        "-m",
        "sec_edgar_mcp.server"
      ],
      "env": {
        "SEC_EDGAR_USER_AGENT": "${SEC_EDGAR_USER_AGENT}",
        "SEC_EDGAR_RATE_LIMIT": "8",
        "SEC_EDGAR_TIMEOUT": "120"
      },
      "disabled": false
    },
    "yahoo-finance": {
      "command": "python",
      "args": [
        "-m",
        "mcp_server.yahoo_finance_server"
      ],
      "disabled": false
    },
    "tradingview": {
      "command": "python",
      "args": [
        "-m",
        "mcp_server.tradingview.server"
      ],
      "disabled": false,
      "env": {
        "TRADINGVIEW_RATE_LIMIT": "10",
        "TRADINGVIEW_TIMEOUT": "30"
      }
    }
  }
}