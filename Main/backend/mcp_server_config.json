{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": [
        "@modelcontextprotocol/server-filesystem",
        "/app"
      ],
      "disabled": false
    },
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp",
        "--config",
        "/app/playwright_mcp_config.json"
      ],
      "disabled": false
    },
    "sec-edgar": {
      "command": "python",
      "args": [
        "-m",
        "sec_edgar_mcp.server"
      ],
      "env": {
        "SEC_EDGAR_USER_AGENT": "${SEC_EDGAR_USER_AGENT}",
        "SEC_EDGAR_RATE_LIMIT": "8",
        "SEC_EDGAR_TIMEOUT": "120"
      },
      "disabled": false
    }
  }
}